<form bindsubmit="formSubmit">
  <view class="setting">
      <input style="margin-left:30rpx;" value="{{form.name}}" name="name" placeholder="请输入姓名"></input>
  </view>
    <view class="setting">
      <input style="margin-left:30rpx;" value="{{form.studentId}}" name="studentId" placeholder="请输入学号" type="number"></input>
  </view>
    <view class="setting">
      <input style="margin-left:30rpx;" value="{{form.school}}" name="school" placeholder="请输入学院"></input>
  </view>
  <view class="setting">
      <input style="margin-left:30rpx;" value="{{form.major}}" name="major" placeholder="请输入专业"></input>
  </view>
  <view class="setting">
      <input style="margin-left:30rpx;" value="{{form.grade}}" name="grade" placeholder="请输入年级" type="number"></input>
  </view>
  <view class="setting">
      <input style="margin-left:30rpx;" value="{{form.myclass}}" name="myclass" placeholder="请输入班级" type="number"></input>
  </view>
  <button class='submitbutton' form-type="submit">提交</button>
</form>