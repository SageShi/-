.setting {
  height: 50px;
  width: 100%;
  background: #fff;
  border-bottom: 1px solid rgba(0, 0, 0, 0.1);
  display: flex;
  transition: all 300ms ease;
  padding: 0 0 0 20rpx;
  justify-items: flex-start;
  box-sizing: border-box;
  font-family:cursive;
  align-items: center;
  flex-direction: row;
}
.submitbutton{
  width: 80%;
  background-color: #efb336;
  color: white;
  border-radius: 98rpx;
  margin-top: 50px;
}